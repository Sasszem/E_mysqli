    , "homepage"    : "https://github.com/noahheck/E_mysqli"
    , "license"     : "Apache-2.0"
    , "authors"     : [
        {
              "name"     : "Noah Heck"
            , "role"     : "Developer/Project Manager"
            , "email"    : "myesain84@gmail.com"
            , "homepage" : "https://github.com/noahheck"
        }
    ]
    , "require"     : {
        "php"                   : ">=5.6.0"
    }
    , "require-dev" : {
